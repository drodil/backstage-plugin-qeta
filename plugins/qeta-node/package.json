    "plugin",
    "backstage.io",
    "node"
  ],
  "version": "3.28.2",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "prepublishOnly": "yarn tsc && yarn build",
  "license": "MIT",
  "homepage": "https://github.com/drodil/backstage-plugin-qeta",
  "bugs": {
    "url": "https://github.com/drodil/backstage-plugin-qeta/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drodil/backstage-plugin-qeta.git"
  },
  "publishConfig": {
